@model List<Category>

<h1>Category LIst</h1>
<table class="table table-bordered table-striped">
	<thead>
		
	<tr>
		<th>Category Name</th>
		<th>Display Order</th>
	</tr>
	</thead>
	<tbody>
		@foreach (var category in Model)
		{
			<tr>
				<td>@category.Name</td>
				<td>@category.DisplayOrder</td>
			</tr>
		}
		
</table>